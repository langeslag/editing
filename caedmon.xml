  <text>
    <body>
      <div type="edition">
        <div type="poem" n="1" xml:id="caedmonshymn">
          <lg>
            <l n="1">Nu we <app>
                <lem wit="#C">herigan sculon</lem>
                <rdg wit="#L">sculon herian <del rend="underlining">herian</del></rdg>
              </app><caesura/> heofonrices weard</l>
            <l n="2">metodes mihte<caesura/> &et; his modgeþanc</l>
            <l n="3">weorc <app>
                <lem wit="#C">wuldorgodes</lem>
                <rdg wit="#L">wulderfæder</rdg>
              </app><caesura/> swa he wundra <app>
                <lem wit="#C">fela</lem>
                <rdg wit="#L">gehwæs</rdg>
              </app></l>
            <l n="4">ece drihten <caesura/>
              <app>
                <lem wit="#C">ord astealde</lem>
                <rdg wit="#L"/>
              </app></l>
            <l n="5"><app>
                <lem wit="#C">he</lem>
                <rdg wit="#L">þa he</rdg>
              </app> ærest sceop <caesura/>
              <app>
                <lem wit="#C">eorðan</lem>
                <rdg wit="#L">eorðe</rdg>
              </app> bearnum</l>
            <l n="6">heofon to hrofe <caesura/>
              <app>
                <lem wit="#C #L">halig scyppend</lem>
                <witDetail wit="#L">transposed to end of poem</witDetail>
              </app></l>
            <l n="7"><app>
                <lem wit="#C">þe</lem>
                <rdg wit="#L">þa</rdg>
              </app> middangeard<caesura/> manncynnes weard</l>
            <l n="8">ece drihten<caesura/> æfter teode</l>
            <l n="9">fyrum <app>
                <lem wit="#C">foldan</lem>
                <rdg wit="#L">on folden</rdg>
                <note>Here's a <mentioned>note</mentioned>.</note>
              </app><caesura/> frea ælmihtig</l>
          </lg>
        </div>
      </div>
    </body>
  </text>
